class Note(object):
    def __init__(self, duration, image, pitch="C5"):
        self.duration = duration
        self.pitch = pitch
        self.image = image
    def playNote(self):
        pass
        # play audio file *pitch* for self.duration time
    # getters and setters
    def getPitch(self):
        return self.pitch
    def setPitch(self, p):
        self.pitch = p
    def getDuration(self):
        return self.duration
